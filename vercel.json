{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "builds": [
    {
      "src": "/templates/index.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "/app.py",
      "use": "@vercel/python"
    }
  ]
}
